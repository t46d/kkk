{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/sitemap.xml",
      "dest": "server.js"
    },
    {
      "src": "/robots.txt",
      "dest": "server.js"
    },
    {
      "src": "/humans.txt",
      "dest": "server.js"
    },
    {
      "src": "/admin",
      "dest": "server.js"
    },
    {
      "src": "/platform",
      "dest": "server.js"
    },
    {
      "src": "/coming-soon.html",
      "dest": "server.js"
    },
    {
      "src": "/admin.html",
      "dest": "server.js"
    },
    {
      "src": "/index.html",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_URI": "@vexachat-mongodb-uri"
  },
  "functions": {
    "server.js": {
      "maxDuration": 10
    }
  }
}
